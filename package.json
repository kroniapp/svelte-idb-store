{
	"name": "svelte-idb-store",
	"version": "0.2.2",
	"description": "A library that allows you to sync the value of your Svelte stores with IndexedDB",
	"keywords": [
		"svelte",
		"svelte-store",
		"idb",
		"indexeddb"
	],
	"homepage": "https://github.com/kroniapp/svelte-idb-store",
	"author": {
		"name": "Kronia",
		"email": "kroniapp@gmail.com"
	},
	"bugs": {
		"url": "https://github.com/kroniapp/svelte-idb-store/issues",
		"email": "kroniapp@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kroniapp/svelte-idb-store"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"type": "module",
	"browser": true,
	"scripts": {
		"start": "vite dev",
		"build": "svelte-package"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.6.1",
		"@sveltejs/package": "^2.3.5",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "5.0.0-next.260",
		"typescript": "^5.6.2",
		"vite": "5.4.8"
	},
	"dependencies": {
		"idb": "^8.0.0"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"default": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./v5": {
			"types": "./dist/v5.svelte.d.ts",
			"default": "./dist/v5.svelte.js",
			"svelte": "./dist/v5.svelte.js"
		}
	}
}